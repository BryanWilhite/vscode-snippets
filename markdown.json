{
    "Insert block: cSpell disable": {
        "prefix": "cSpell disable",
        "body": [
            "<!-- cSpell:disable -->",
            "$0",
            "<!-- cSpell:enable -->"
        ],
        "description": "Insert block: cSpell disable"
    },
    "Insert block: div text centering with image": {
        "prefix": "div text centering with image",
        "body": [
            "<div style=\"text-align:center\">",
            "",
            "![${1:caption}](http://kintespace.com/bitmaps/$0 \"${1:caption}\")",
            "",
            "</div>"
        ],
        "description": "Insert block: div text centering with image"
    },
    "Insert block: image with anchor": {
        "prefix": "image with anchor",
        "body": [
            "<!-- cSpell:disable -->",
            "<a href=\"${1:href}\" target=\"_blank\"><img alt=\"${2:alt}\" src=\"${3:src}\" style=\"float:$0right;margin:16px;\" /></a>",
            "<!-- cSpell:enable -->"
        ],
        "description": "Insert block: image with anchor"
    },
    "Insert block: span with float": {
        "prefix": "span with float",
        "body": [
            "<span style=\"float:right;margin:16px\">",
            "",
            "$0",
            "",
            "</span>"
        ],
        "description": "Insert block: span with float"
    },
    "Insert block: title as comment": {
        "prefix": "title as comment",
        "body": [
            "<!--",
            "    title: $0",
            "-->"
        ],
        "description": "Insert block: title as comment"
    },
    "Insert block: YouTube embed": {
        "prefix": "YouTube embed",
        "body": [
            "<!-- cSpell:disable -->",
            "<iframe width=\"560\" height=\"315\" src=\"https://www.youtube.com/embed/$0\" frameborder=\"0\" allowfullscreen style=\"display: block; margin: 0 auto;\"></iframe>",
            "<!-- cSpell:enable -->"
        ],
        "description": "Insert block: YouTube embed"
    },
     "Insert entity: non-breaking space": {
        "prefix": "nbsp",
        "body": [
            "&#160;$0"
        ],
        "description": "Insert entity: non-breaking space"
    },
