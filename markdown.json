{
    "Insert block: cSpell disable": {
        "prefix": "cSpell disable",
        "body": ["<!-- cSpell:disable -->", "$0", "<!-- cSpell:enable -->"],
        "description": "Insert block: cSpell disable"
    },
    "Insert block: div text centering": {
        "prefix": "div text centering",
        "body": ["<div style=\"text-align:center\">", "", "$0", "", "</div>"],
        "description": "Insert block: div text centering"
    },
    "Insert block: image with anchor": {
        "prefix": "image with anchor",
        "body": [
            "<!-- cSpell:disable -->",
            "<a href=\"${1:href}\" target=\"_blank\"><img alt=\"${2:alt}\" src=\"${3:src}\" /></a>",
            "<!-- cSpell:enable -->"
        ],
        "description": "Insert block: image with anchor"
    },
    "Insert block: image with anchor with float": {
        "prefix": "image with anchor with float",
        "body": [
            "<!-- cSpell:disable -->",
            "<a href=\"${1:href}\" target=\"_blank\"><img alt=\"${2:alt}\" src=\"${3:src}\" style=\"float:$0right;margin:16px;\" /></a>",
            "<!-- cSpell:enable -->"
        ],
        "description": "Insert block: image with anchor with float"
    },
    "Insert block: span with float": {
        "prefix": "span with float",
        "body": [
            "<span style=\"float:right;margin:16px\">",
            "",
            "$0",
            "",
            "</span>"
        ],
        "description": "Insert block: span with float"
    },
    "Insert block: title as comment": {
        "prefix": "title as comment",
        "body": ["<!--", "    title: $0", "-->"],
        "description": "Insert block: title as comment"
    },
    "Insert entity: non-breaking space": {
        "prefix": "nbsp",
        "body": ["&#160;$0"],
        "description": "Insert entity: non-breaking space"
    },
    "Insert glyphs: apostrophe": {
